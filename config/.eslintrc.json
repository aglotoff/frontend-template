{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jquery": true
    },
    "extends": [
        "airbnb-base",
        "plugin:import/errors",
        "plugin:import/warnings"
    ],
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus": "off",
        "import/prefer-default-export": "off",
        "no-use-before-define": "off",
        "no-new": "off",
    }
}